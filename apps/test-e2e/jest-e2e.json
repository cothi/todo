{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@user/(.*)$": "<rootDir>/../user/src/$1",
    "^@auth/(.*)$": "<rootDir>/../auth/src/$1",
    "^@project/(.*)$": "<rootDir>/../project/src/$1",
    "^@libs/(.*)$": "<rootDir>/../../libs/$1/src"
  },
  "modulePaths": ["<rootDir>/../../"],
  "setupFiles": ["<rootDir>/jest-e2e.setup.ts"]
}
